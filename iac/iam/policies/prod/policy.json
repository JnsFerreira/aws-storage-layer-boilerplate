{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowGetOperationsToAccount",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::123456789012:root"
            },
            "Action": [
                "s3:Get*"
            ],
            "Resource": [
                "arn:aws:s3:::my-prod-bucket/*"
            ]
        }
    ]
}